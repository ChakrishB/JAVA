public class SkipNumbers {
    public static void main(String[] args) {
        int m = 50;
        int n = 100;
        int k = 7;

        printNumbers(m, n, k);
    }

    public static void printNumbers(int m, int n, int k) {
        for (int i = m; i <= n; i += k) {
            System.out.print(i + ", ");
        }
    }
}
